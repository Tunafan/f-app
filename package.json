{
  "name": "f-app",
  "version": "0.0.1",
  "description": "Fisher's best friend in an app built on Quasar, Vue, and Supabase using edge functions",
  "productName": "F-App",
  "author": "Tunafan <124045808+Tunafan@users.noreply.github.com>",
  "type": "module",
  "private": true,
    "scripts": {
    "lint": "eslint . --cache",
    "generate-translations": "node ./src-translator/index.js",
    "dev": "cross-env NODE_ENV=development quasar dev",
    "dev:analyze": "cross-env NODE_ENV=development ANALYZE=true quasar dev",
    "dev:install": "cd ./plugins/capacitor-firebase-mfa && npm ci && npm run build && cd ../../ && cd ./plugins/capacitor-android-store-provider && npm ci && npm run build && cd ../../ && cd ./src-capacitor && npm ci && cd ../ && cd ./functions && npm ci && cd ../ && npm ci",
    "build:prod": "cross-env NODE_ENV=production quasar build -m pwa && cross-env NODE_ENV=production node ./scripts/swEnvBuild.js",
    "build:staging": "cross-env NODE_ENV=staging quasar build -m pwa && cross-env NODE_ENV=staging node ./scripts/swEnvBuild.js",
    "build:analyze": "cross-env NODE_ENV=production ANALYZE=true quasar build -m pwa && cross-env NODE_ENV=production node ./scripts/swEnvBuild.js",
    "deploy:hosting": "firebase deploy --only hosting",
    "deploy:rules": "npm run deploy:firestore:rules && npm run deploy:storage:rules",
    "deploy:firestore:rules": "firebase deploy --only firestore:rules",
    "deploy:storage:rules": "firebase deploy --only storage",
    "deploy:indexes": "firebase deploy --only firestore:indexes",
    "fetch:staging:indexes": "firebase --project hi-rasmus-staging firestore:indexes > ./firestore/firestore.indexes.json",
    "release": "sh scripts/deploy/deploy_new_version.sh",
    "postinstall": "patch-package",
    "test:unit": "jest",
    "test:unit:ui": "npx majestic --port 4100",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll"
  },
